{
  "name": "@world-boss/web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "prisma generate && prisma db push --skip-generate && next build",
    "start": "NODE_ENV=production node server.js",
    "lint": "next lint",
    "db:push": "prisma db push",
    "db:push:force": "prisma db push --accept-data-loss",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "bot:setup": "node scripts/setup-bot.js",
    "bot:webhook": "node scripts/set-webhook.js"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.300.0",
    "next": "^14.1.0",
    "phaser": "^3.90.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "prisma": "^5.8.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0"
  }
}
